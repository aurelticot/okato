{
  "name": "okato-web-client",
  "description": "Web client of the Okato application",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "extractMessages": "formatjs extract 'src/**/*.ts*' --out-file messages/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compileMessages": "formatjs compile-folder --ast messages/ src/lang/"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.0",
    "@formatjs/cli": "^2.7.3",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/luxon": "^1.24.3",
    "@types/node": "^12.12.54",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "apollo-boost": "^0.4.9",
    "graphql": "^15.3.0",
    "luxon": "^1.24.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^5.4.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "typescript": "^3.7.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:9000"
}
